{
  "name": "gacm",
  "version": "1.0.0",
  "description": "gacm",
  "scripts": {
    "build": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts",
    "clear": "rimraf dist",
    "link": "pnpm link --global",
    "push": "git push gitee master && git push github master",
    "update:version": "sucrase-node build/utils/version.ts",
    "log": "conventional-changelog -p custom-config  -i CHANGELOG.md -s -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "bin": {
    "gacm": "./dist/main.js"
  },
  "repository": {
    "url": "https://github.com/alqmc/gacm.git"
  },
  "author": "alqmc",
  "license": "MIT",
  "devDependencies": {
    "@alqmc/build-ts": "^0.0.8",
    "@alqmc/build-utils": "^0.0.3",
    "@alqmc/eslint-config": "0.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/node": "^17.0.21",
    "commitizen": "^4.1.2",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "sucrase": "^3.20.3",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "commander": "^9.3.0",
    "execa": "5.0.1",
    "kolorist": "^1.5.1",
    "minimist": "^1.2.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}